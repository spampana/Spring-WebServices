<?xml version="1.0" encoding="UTF-8"?>
<mapping>
        <class name="org.krams.tutorial.oxm.SubscriptionRequest">
           <map-to xml="subscriptionRequest" ns-uri="http://krams915.blogspot.com/ws/schema/oss"/>
           
           <field name="id" type="string">
              <bind-xml name="id" node="element"/>
           </field>
           
           <field name="name" type="string">
              <bind-xml name="name" node="element"/>
           </field>
           
           <field name="email" type="string">
              <bind-xml name="email" node="element"/>
           </field>
        </class>
        
        <class name="org.krams.tutorial.oxm.SubscriptionResponse">
           <map-to xml="subscriptionResponse" ns-uri="http://krams915.blogspot.com/ws/schema/oss"/>
      
           <field name="code" type="string">
              <bind-xml name="code" node="element"/>
           </field>
           
           <field name="description" type="string">
              <bind-xml name="description" node="element"/>
           </field>
        </class>
</mapping>